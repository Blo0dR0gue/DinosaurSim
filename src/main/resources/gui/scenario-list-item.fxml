<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<fx:root type="javafx.scene.layout.HBox" prefHeight="30.0" fx:id="container" alignment="CENTER_LEFT"
         xmlns="http://javafx.com/javafx/11.0.2"
         xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0"/>
    </padding>
    <RadioButton fx:id="radioButton" text="ScenarioName" textFill="white">
        <HBox.margin>
            <Insets right="10.0"/>
        </HBox.margin>
        <font>
            <Font size="17.0"/>
        </font>
    </RadioButton>
</fx:root>
