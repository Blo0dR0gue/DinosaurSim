<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="hbox_outer" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../dinosaur/test.png" />
         </image>
      </ImageView>
      <BorderPane fx:id="borderPane" prefHeight="200.0" prefWidth="600.0" HBox.hgrow="ALWAYS">
         <top>
            <Label fx:id="label_name" text="PLATZHALTER" BorderPane.alignment="CENTER">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </top>
         <center>
            <Slider fx:id="slider" majorTickUnit="10.0" minorTickCount="1" showTickLabels="true" showTickMarks="true" snapToTicks="true" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <HBox fx:id="hbox_inner" prefHeight="0.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="button_info" mnemonicParsing="false" text="Info" />
                  <Button fx:id="button_remove" mnemonicParsing="false" text="Remove" />
               </children>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</HBox>
