<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import javafx.geometry.Insets?>
<fx:root type="javafx.scene.layout.HBox" fx:id="root" prefHeight="50.0" alignment="CENTER_LEFT"
         xmlns="http://javafx.com/javafx/11.0.2"
         xmlns:fx="http://javafx.com/fxml/1">
   <ImageView fx:id="imageView" fitHeight="30.0" fitWidth="30.0" preserveRatio="true">
      <Image url="@../dinosaur/test.png"/>
   </ImageView>
   <Label fx:id="legendLabel" text="= DinoName" textFill="white">
      <HBox.margin>
         <Insets left="3.0"/>
      </HBox.margin>
      <font>
         <Font size="12.0"/>
      </font>
   </Label>
</fx:root>
